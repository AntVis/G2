{
  "extends": ["egg"],
  "globals": {
    "$": true,
    "AMap": true,
    "AMapUI": true,
    "DataSet": true,
    "G2": true,
    "_": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["html"],
  "rules": {
    "comma-dangle": 0,
    "no-console": [
      "error",
      {
        "allow": ["warn", "error"]
      }
    ],
    "no-bitwise": [0],
    "experimentalDecorators": [0],
    "linebreak-style": [0],
    "array-bracket-spacing": 0,
    "curly": 0,
    "one-var-declaration-per-line": 0
  }
}
